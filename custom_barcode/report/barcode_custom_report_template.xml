
<odoo> 
    <data>
        <report
                id="report_print_custom_product_barcode"
                model="product.label.layout"
                report_type="qweb-pdf"
                string="Product Label (PDF)" 
                name="custom_barcode.barcode_custom_report_template"
                file="custom_barcode.barcode_custom_report_template"
                paperformat="product.paperformat_label_sheet_dymo"
            />

      
        <template id="barcode_custom_report_template">
            <t t-call="web.basic_layout">
              <div class="page">
                            <t t-foreach="range(custom_quantity)" t-as="qty">
                                
                                <t t-foreach="docs._prepare_data(active_model,ids)" t-as="product">
                                
                                        
                                        <div class="row" style="height: 100%;">
                                            <div class="column" style="float: left;text-align:center; width:100%;height: 100%;">
                                                <t t-esc="product.name"/>
                                            </div>
                                        </div>
                                </t>
                            </t>
                    
              
              </div>
            </t>
                
                    
                
                    
        </template>
        
    </data>
</odoo>