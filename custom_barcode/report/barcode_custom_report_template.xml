
<odoo> 
    <data>
        <report
                id="report_print_custom_product_barcode"
                model="product.label.layout"
                report_type="qweb-pdf"
                string="Product Label (PDF)" 
                name="custom_barcode.barcode_custom_report_template"
                file="custom_barcode.barcode_custom_report_template"
                paperformat="product.paperformat_label_sheet_dymo"
            />

      
        <template id="barcode_custom_report_template">
            <t t-call="web.basic_layout">
              <div class="page">
                            <t t-foreach="range(custom_quantity)" t-as="qty">
                                <t t-foreach="docs._prepare_data(active_model,ids)" t-as="product">
                                        
                                        
                                        <div class="row">
                                            <div class="col-12" style="text-align:center; height:95px;">
                                              <div class="row">
                                                  <div class="col-12"  style="margin-top:5px;">
                                                    <div t-if="product.barcode" t-field="product.barcode" t-options="{'widget': 'barcode', 'humanreadable': 0, 'width': 315, 'height': 52}"/>
                                                  </div>
                                                </div>
                                                <div class="row">
                                                  <div class="col-12"   style="font-size:11x;">
                                                    <t t-esc="product.name"/>
                                                  </div>
                                                </div>
                                                <div class="row">
                                                  <div class="col-12"  style="font-size:11px;" >
                                                    <t t-esc="product.put_away_location.name"/>
                                                  </div>
                                                </div>
                                          

                                               
                                            </div>
                                        </div>
                                </t>
                            </t>
                    
              
              </div>
            </t>
                
                    
                
                    
        </template>
        
    </data>
</odoo>